{
  "manifest_version": 3,
  "name": "Shrute Quotes",
  "version": "1.1.0",
  "description": "Open a new tab, get Dwight Shrute wisdom.",
  "icons": {
    "16": "assets/icon.png",
    "32": "assets/icon.png",
    "48": "assets/icon.png",
    "64": "assets/icon.png",
    "128": "assets/icon128.png"
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["app.js"]
  }]
}